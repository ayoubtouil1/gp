function selectPlatform(plateform){
  document.getElementById("platform1").classList.remove("sakeanimation");
  document.getElementById("platform2").classList.remove("sakeanimation");
  document.getElementById("platform3").classList.remove("sakeanimation");
  document.getElementById("platform1").style.backgroundColor="#365b6a";
  document.getElementById("platform2").style.backgroundColor="#365b6a";
  document.getElementById("platform3").style.backgroundColor="#365b6a";
  document.getElementById("platform1").style.borderColor="#578ba1";
  document.getElementById("platform2").style.borderColor="#578ba1";
  document.getElementById("platform3").style.borderColor="#578ba1";

  document.getElementById("platform"+plateform).classList.add("sakeanimation");
document.getElementById("platform"+plateform).style.backgroundColor="#417dbe";
document.getElementById("platform"+plateform).style.borderColor="#ffff";
plateformIndex=plateform;
console.log(document.getElementById("platform"+plateform).style.backgroundImage);
document.getElementById("seletedplatform").style.backgroundImage=document.getElementById("platform"+plateform).style.backgroundImage;
}

function cardOver(cardnum){
    if (cardclicked!=cardnum){
    document.getElementById("card"+cardnum).style.backgroundColor="yellow";
        document.getElementById("card"+cardnum).classList.remove("zoomin");
    document.getElementById("card"+cardnum).classList.add("zoomout");
  }
}
function cardOut(cardnum){
    if (cardclicked!=cardnum){
    document.getElementById("card"+cardnum).style.backgroundColor="white";
        document.getElementById("card"+cardnum).classList.remove("zoomout");
        document.getElementById("card"+cardnum).classList.add("zoomin");
      }
}
function selectedcard(cardnum){
    document.getElementById("bntproceed1").style.display="block";
      document.getElementById("card1").classList.remove("selectedcard");
      document.getElementById("card2").classList.remove("selectedcard");
      document.getElementById("card3").classList.remove("selectedcard");
      document.getElementById("card4").classList.remove("selectedcard");
      document.getElementById("card1").classList.add("zoomin");
      document.getElementById("card2").classList.add("zoomin");
      document.getElementById("card3").classList.add("zoomin");
      document.getElementById("card4").classList.add("zoomin");
document.getElementById("card"+cardnum).classList.remove("zoomout");
document.getElementById("card"+cardnum).classList.remove("zoomin");
  document.getElementById("card"+cardnum).classList.add("selectedcard");
    cardclicked=cardnum;
  }
